{
  "name": "gjump",
  "displayName": "gjump",
  "description": "Yet another EasyMotion inspired jumper",
  "version": "0.0.3",
  "repository": "https://github.com/nonpop/gjump",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gjump.initJump",
        "title": "gjump: initiate jump"
      },
      {
        "command": "gjump.initMultiJump",
        "title": "gjump: initiate multi cursor jump"
      },
      {
        "command": "gjump.initSelect",
        "title": "gjump: initiate select"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
